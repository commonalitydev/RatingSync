{
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "manifest_version": 2,

  "name": "RatingSync",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0.16",

  "browser_action": {
    "default_icon": "iconBeta.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ]
}