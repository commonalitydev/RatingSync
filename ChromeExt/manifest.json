{
  "manifest_version": 2,

  "name": "RatingSync",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://192.168.1.105:55887/",
    "http://google.com/",
    "https://ajax.googleapis.com/"
  ]
}